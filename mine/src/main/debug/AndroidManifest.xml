<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ericshenn.mine">

    <application
        android:name="debug.MineApplication"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".mine.MineActivity" />
        <activity android:name=".mycollection.MyCollectionActivity" />
        <activity android:name=".setup.SetUpActivity" />
        <activity android:name="debug.activity.DebugActivity">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />

                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
    </application>

</manifest>
